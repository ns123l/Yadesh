<%if(success && success.length){%>
    <div class="alert alert-success" role="alert">
       <%=success%>
       <button type="button" class="btn-close alert-dismissible fade show" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    <%}%>
 <%if(error && error.length){%>
    <div class="alert alert-success" role="alert">
      <%=error%>
         <button type="button" class="btn-denger alert-dismissible fade show" data-bs-dismiss="alert" aria-label="Close"></button>
     </div>
 <%}%>